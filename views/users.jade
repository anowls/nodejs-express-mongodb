extends layout

block content
    nav(class="navbar navbar-default")
        div(class="container-fluid")
            div(class="navbar-header")
                button(type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#collpse")
            div(class="navbar-collapse collapse" id="collpse")
                ul(class="nav navbar-nav")
                    li
                        a action
                    li(class="dropdown")
                       a(href='#' class="dropdown-toggle" data-toggle="dropdown") Dropdown&nbsp;
                            span.caret
                       ul(class="dropdown-menu")
                           li
                               a(href="#") action
                           li
                               a(href="#") action
                           li
                               a(href="#") action
                           li
                               a(href="#") action
    ul.list-group
        each result in results
            li.list-group-item
                a.list-group-item-danger(href="#") #{result._id}&nbsp;#{result.content}&nbsp;#{result.pratice}